\begin_layout Plain Layout
\begin_inset Formula 
\begin{equation}
C_{Ga}^{i+1,j}C_{As}^{i+1,j}\approx C_{Ga}^{i,j}C_{As}^{i+1,j}+C_{Ga}^{i+1,j}C_{As}^{i,j}-C_{Ga}^{i,j}C_{As}^{i,j}
\end{equation}

\end_inset


\end_layout

\begin_layout Plain Layout
Note that the full term according to our scheme should be:
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{1}{2}\left(C_{Ga}^{i+1,j}C_{As}^{i+1,j}+C_{Ga}^{i,j}C_{As}^{i,j}\right)=\frac{1}{2}\left(C_{Ga}^{i,j}C_{As}^{i+1,j}+C_{Ga}^{i+1,j}C_{As}^{i,j}\right)
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
Finite difference equations should look like:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{for }j=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{Ga}^{i+1,0}-\frac{\Delta t}{2}\left(\frac{D_{Ga}}{\Delta r^{2}}\left[4C_{Ga}^{i+1,1}-4C_{Ga}^{i+1,0}\right]-k_{r}C_{Ga}^{i+1,0}C_{As}^{i,0}-k_{r}C_{Ga}^{i,0}C_{As}^{i+1,0}\right)=C_{Ga}^{i,0}+\frac{\Delta t}{2}\left(\frac{D_{Ga}}{\Delta r^{2}}\left[4C_{Ga}^{i,1}-4C_{Ga}^{i,0}\right]+F_{Ga}^{i+1,0}+F_{Ga}^{i,0}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{As}^{i+1,0}-\frac{\Delta t}{2}\left(\frac{D_{As}}{\Delta r^{2}}\left[4C_{As}^{i+1,1}-4C_{As}^{i+1,0}\right]-\frac{C_{As}^{i+1,0}}{\tau_{As}}-k_{r}C_{Ga}^{i+1,0}C_{As}^{i,0}-k_{r}C_{Ga}^{i,0}C_{As}^{i+1,0}\right)=C_{As}^{i,0}+\frac{\Delta t}{2}\left(\frac{D_{As}}{\Delta r^{2}}\left[4C_{As}^{i,1}-4C_{As}^{i,0}\right]-\frac{C_{As}^{i,0}}{\tau_{As}}+2F_{As}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{for }j>0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
C_{Ga}^{i+1,j}-\frac{\Delta t}{2}\left(\frac{D_{Ga}}{\Delta r^{2}}\left[\left(1+\frac{1}{2j}\right)C_{Ga}^{i+1,j+1}-2C_{Ga}^{i+1,j}+\left(1-\frac{1}{2j}\right)C_{Ga}^{i+1,j-1}\right]-k_{r}\left(C_{Ga}^{i,j}C_{As}^{i+1,j}+C_{Ga}^{i+1,j}C_{As}^{i,j}\right)\right)=\\
=C_{Ga}^{i,j}+\frac{\Delta t}{2}\left(\frac{D_{Ga}}{\Delta r^{2}}\left[\left(1+\frac{1}{2j}\right)C_{Ga}^{i,j+1}-2C_{Ga}^{i,j}+\left(1-\frac{1}{2j}\right)C_{Ga}^{i,j-1}\right]+F_{Ga}^{i+1,j}+F_{Ga}^{i,j}\right)
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
C_{As}^{i+1,j}-\frac{\Delta t}{2}\left(\frac{D_{As}}{\Delta r^{2}}\left[\left(1+\frac{1}{2j}\right)C_{As}^{i+1,j+1}-2C_{As}^{i+1,j}+\left(1-\frac{1}{2j}\right)C_{As}^{i+1,j-1}\right]-\frac{C_{As}^{i+1,j}}{\tau_{As}}-k_{r}\left(C_{Ga}^{i,j}C_{As}^{i+1,j}+C_{Ga}^{i+1,j}C_{As}^{i,j}\right)\right)=\\
=C_{As}^{i,j}+\frac{\Delta t}{2}\left(\frac{D_{As}}{\Delta r^{2}}\left[\left(1+\frac{1}{2j}\right)C_{As}^{i,j+1}-2C_{As}^{i,j}+\left(1-\frac{1}{2j}\right)C_{As}^{i,j-1}\right]-\frac{C_{As}^{i,j}}{\tau_{As}}+2F_{As}\right)
\end{multline*}

\end_inset


\end_layout

